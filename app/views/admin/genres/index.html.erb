<h2>ジャンル一覧追加</h2> 
 <%= form_with model: @genre, url: admin_genres_path do |f| %>
 <h4>ジャンル名</h4>
　<%= f.text_field :name %>
　 <%= f.submit '新規登録', class: "btn btn-sn btn-success" %>
 <% end %>
 
 
<table class="table table-hover table-invrse">
  <thead>
   <tr>
    <th>ジャンル</th>
   </tr>
  </thead>
 　   <% @genres.each do |genre| %>
  <tbody>
   <tr>
    <td><%= genre.name %></td>
    <td><%= link_to "編集する", edit_admin_genre_path(genre.id), class: "btn btn-sn btn-success" %></td>
   </tr>
  </tbody>
 </table> 
 <% end %>