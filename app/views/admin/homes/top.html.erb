<h4>投稿一覧</h4>
<table class="table table-hover table-invrse">
  <thead>
   <tr>
    <th>投稿id</th>
    <th>お酒の名前</th>
    <th>投稿日時</th>
    <th>投稿者の名前</th>
   </tr>
  </thead>
  
  <tbody>
   <% @posts.each do |post| %>
   <tr>
    <td><%= post.id %></td>
    <td><%= link_to post.liquor_name, admin_post_path(post.id) %></td>
    <td><%= link_to post.created_at.strftime('%Y/%m/%d'), admin_post_path(post.id) %></td>
    <td><%= link_to post.customer.name, admin_customer_path(post.customer) %></td>
   </tr>
      
    <% end %>
  </tbody>
</table>

 